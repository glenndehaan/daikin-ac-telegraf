{
  "name": "daikin-ac-telegraf",
  "version": "0.0.0",
  "description": "",
  "main": "./src/index.js",
  "bin": "./src/index.js",
  "scripts": {
    "prestart": "rimraf ./dist",
    "start": "nodemon --watch src --exec 'npm run lint && node ./src/index.js --'",
    "prebuild": "rimraf ./dist",
    "build": "pkg ./package.json --targets latest-linux-x64,latest-macos-x64,latest-win-x64 --out-path ./dist",
    "lint": "eslint -c ./package.json ./"
  },
  "engines": {
    "node": "^14.0.0"
  },
  "author": "Glenn de Haan",
  "license": "MIT",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-console": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "indent": [
        "error",
        4
      ]
    },
    "extends": [
      "eslint:recommended"
    ]
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "nodemon": "^2.0.7",
    "pkg": "^4.4.9",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "daikin-controller": "^1.2.0"
  }
}
